<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoFinal.Login">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese el Usuario"/>
            <Entry x:Name="txtConstrasenia" Placeholder="Ingrese la Contraseña" IsPassword="True"/>
            <Button x:Name="btnIniciar" Text="Iniciar Sesión" Clicked="btnIniciar_Clicked"/>
            <Button x:Name="btnRegistrar" Text="Registrarse" Clicked="btnRegistrar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>